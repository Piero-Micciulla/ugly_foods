<%= simple_form_for(@shop) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :post_code %>
  <%= f.number_field :post_code %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :website %>
  <%= f.text_field :website %>
  <%= f.label :phone_number %>
  <%= f.number_field :phone_number %>
  <%= f.label :photo %>
  <%= f.input :photo, as: :file %>
<% end %>
